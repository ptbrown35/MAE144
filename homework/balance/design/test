

b[3] // [b2,b1,b0]
a[3] // [a2,a1,a0]
u[3] // [uk,uk-1,uk-2]
y[3] // [yk,yk-1,yk-2]

for(int i = 0; i < 3; i++){
  b[i] = b[i] / a[0];
  a[i] = a[i] / a[0];
}

for(int i = 0; i < 3; i++){
  y[0] += b[i] * u[i]
}

for(int i = 1; i < 3; i++){
  y[0] -= a[i] * y[i]
}

print(y[0])


float tf(const float[3] a, const float[3] b, const float[3] u, const float[3] y,){
  // Compute y[0] for b
  for(int j = 0; j < 3; j++){
    y[0] += b[j] * u[j];
  }

  // Compute y[0] for a
  for(int k = 1; k < 3; k++){
    y[0] -= a[k] * y[k];
  }

  // return newest output
  return y[0];
}
